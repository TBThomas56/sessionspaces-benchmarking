{
  "name": "sessionspaces-benchmarking",
  "image": "docker.io/library/python:slim-bookworm",
  "features": {
    "ghcr.io/devcontainers/features/common-utils:2.4.2": {
      "username": "none",
      "upgradePackages": false
    },
    "ghcr.io/georgofenbeck/features/lazygit-linuxbinary:1": {}
},
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-kubernetes-tools.vscode-kubernetes-tools",
        "chaitanyashahare.lazygit"
      ]
    }
  },
  "runArgs": ["--security-opt=label=type:container_runtime_t"],
  "workspaceMount": "source=./,target=/workspace,type=bind",
  "workspaceFolder": "/workspace",
  "postCreateCommand": "pip install --no-cache-dir -r requirements.txt"
}